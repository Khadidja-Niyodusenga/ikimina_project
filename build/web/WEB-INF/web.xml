<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>InsertRequest</servlet-name>
        <servlet-class>MyServlet.InsertRequest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminRequest</servlet-name>
        <servlet-class>MyServlet.AdminRequest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>MyServlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>MyServlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddMember</servlet-name>
        <servlet-class>MyServlet.AddMember</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ApproveRequest</servlet-name>
        <servlet-class>MyServlet.ApproveRequest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertRequest</servlet-name>
        <url-pattern>/InsertRequest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminRequest</servlet-name>
        <url-pattern>/AdminRequest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddMember</servlet-name>
        <url-pattern>/AddMember</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ApproveRequest</servlet-name>
        <url-pattern>/ApproveRequest</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
